<?xml version="1.0" encoding="utf-8"?>
<extension type="plugin" version="2.5" method="upgrade" group="system">
	<name>Emundus - Export to Aurion with user data</name>
	<creationDate>5 December 2019</creationDate>
	<version>6.9.13</version>
	<author>eMundus</author>
	<authorEmail>dev@emundus.fr</authorEmail>
	<authorUrl>https://www.emundus.fr</authorUrl>
	<copyright>(C) 2010-2019 EMUNDUS SOFTWARE. All rights reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<description>This plugin will Export User's data to an External Aurion </description>
	<files>
		<filename plugin="aurion_export">aurion_export.php</filename>
	</files>
	<config>
		<fields name="params">
			<fieldset addfieldpath="/administrator/components/com_emundus/models/fields" name="basic">
				<field name="em_groups" value="" type="usergroup" label="COM_EMUNDUS_USER_GROUPS" description="COM_EMUNDUS_USER_GROUPS_DESC"/>
			</fieldset>
			<fieldset name="basic">
			</fieldset>
		</fields>
	</config>
</extension>
