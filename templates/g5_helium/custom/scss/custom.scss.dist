/* Buttons print & send my file */

.em-print-button a {
  text-shadow: none !important;
  padding: 8px 12px !important;
  border: 1px solid $base-primary-color !important;
  color: #fff !important;
  background: $base-primary-color !important;

  &:hover,
  &:focus,
  &:active {
    border: 1px solid $base-primary-color !important;
    color: $base-primary-color !important;
    background: transparent !important;
  }
}

.send-application-btn {
  padding: 0 !important;
}

.em-send-button a {
  margin-top: 1rem !important;
  background: $base-secondary-color !important;
  text-shadow: none !important;
  width: 100% !important;
  padding: 8px 12px !important;
  font-size: 1rem !important;
  color: #ffffff !important;
  border: 1px solid $base-secondary-color !important;
  display: flex !important;
  justify-content: center;

  &:hover,
  &:focus,
  &:active {
    background: transparent !important;
    color: $base-secondary-color !important;
    border: 1px solid $base-secondary-color !important;
  }
