}

/*MOT DE PASSE OUBLIÉ*/
#user-registration fieldset {
  background: transparent;

  p {
    margin-bottom: 20px !important;
  }
}

ul.nav.nav-tabs.nav-stacked span {
  color: $base-primary-color;
  cursor: pointer;

  &::before {
    content: "\f101";
    font-family: FontAwesome;
