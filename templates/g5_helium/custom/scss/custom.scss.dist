  color: $base-primary-color !important;
  margin: 10px !important;

  &::before {
    height: 105% !important;
  }
}

.swal2-close:focus {
  outline: none !important;
}

/* Inscription */
.registration {
  form {
    background: #4e5b6d;
    border: #4e5b6d;
    margin-top: 40px;
    margin-bottom: 0;
    border-radius: 0;
    color: #ffffff;
  }

  fieldset {
    background: #4e5b6F;
  }

  button {
    display: inline-block;
    font-weight: 600;
    font-size: 12px;
    line-height: 12px;
    letter-spacing: 0.1rem;
    border-radius: 0;
    padding: 8px 12px;
    vertical-align: middle;
    text-shadow: none;
    -webkit-transition: all 0.2s;
    -moz-transition: all 0.2s;
    transition: all 0.2s;
    margin-right: 20px;

    &:hover {
      color: #ffffff;
      background: none;
    }
  }
}

.view-registration .em-register-warning {
  border: 1px solid $base-primary-color;
  border-radius: 0;
  padding: 20px;
  margin: 0 0 30px 0;
}

.em-register-table .em-input input,
