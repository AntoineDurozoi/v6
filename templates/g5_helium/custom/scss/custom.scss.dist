    border: none;
    margin-bottom: 0 !important;
    border-radius: 0;
    padding: 20px !important;
  }

  .well label {
    color: #000;
  }
}

.invalid {
  color: red !important;
  font-weight: 600;
}

ul.nav.nav-tabs.nav-stacked {
  background: #fcfcfc;
  border-radius: 0;
  display: flex;
  justify-content: space-evenly;

  a {
    color: $base-primary-color;

    &:hover,
    &:focus,
    &:active,
    &:focus-within {
      color: $base-primary-color;
      text-decoration: underline;
    }
