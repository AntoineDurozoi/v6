.em-print-button,
.em-send-button {
  display: block;
  width: 100%;
  padding: 0;

  a i {
    margin: 5px 8px !important;
  }
}

/* HEADER */
#g-navigation {
  border-bottom: 1px solid #eeeeee;

  .g-container {
    padding: 10px !important;
  }
}

#g-container-main .g-container {
  padding: 0 10px !important;
}

#header-a {
  display: flex;
  align-items: center;

  .logo {
    max-height: 64px;
  }
}


#header-b {
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.header-right a {
  color: #4E5B6D;
}


/* Burger Menu */
.burger span {
  border-radius: 1px;
  height: 5px;
  width: 32px;
  background: $base-primary-color;
  position: absolute;
  display: block;
  content: '';
  transition: stroke-dasharray 400ms, stroke-dashoffset 400ms;
  top: 0;

  &::before,
  &::after {
    border-radius: 1px;
    height: 5px;
    width: 32px;
    background: $base-primary-color;
    position: absolute;
    display: block;
    content: '';
  }
}

/* USER MENU */
#userDropdownIcon {
  background-color: $base-primary-color !important;
  border: 2px solid $base-primary-color !important;
  color: #ffffff !important;
  box-shadow: none !important;

  &:hover,
  &.active,
  &:hover:active {
    background-color: transparent !important;
    color: $base-primary-color !important;
  }
}

#userDropdownMenu {

  li>a:hover,
  .active>a {
    background: $base-primary-color !important;
    color: #ffffff;
  }
}


/* MENU */
#header-b #em_user_menu {
  a {
    color: #000;
    text-decoration: none;
  }

  li:hover a,
  li:active a,
  li:focus a {
    color: $base-primary-color;
  }
}

.active a.item {
  color: $base-primary-color !important;
}

.g-main-nav .g-toplevel>li.g-parent .g-menu-parent-indicator::after {
  width: auto;
  padding: 0;
  height: auto;
  color: #000;
  opacity: inherit;
}

.g-main-nav .g-toplevel>li.active .g-menu-parent-indicator::after,
.g-main-nav .g-toplevel>li:hover .g-menu-parent-indicator::after {
  color: $base-primary-color !important;
}


ul.g-dropdown.g-dropdown-right.g-fade {
  background: #fff !important;
  border: 1px solid #ddd !important;
  border-radius: 0 !important;

  a {
    color: #000 !important;
  }

  &:hover,
  &:focus,
  &:active,
  &:focus-within {
    color: $base-primary-color !important;
  }
}

.g-main-nav .g-standard .g-sublevel .current a span {
  color: $base-primary-color;
}

.navsearch-content input[type="text"]:focus,
.navsearch-content input[type="text"]:hover {
  border-color: #d5d5d5;
  box-shadow: none;
}

.campaigntext.blue h4 a {
  color: #292929;
}

ul.nav.nav-tabs.nav-stacked span {
  color: $base-primary-color;
  cursor: pointer;

  &:hover,
  &:focus,
  &:active,
  &:focus-within {
    color: $base-primary-color;
    text-decoration: underline;
  }
}

/* Module Campagnes */
.campaign-title {
  color: #292929;
  font-weight: 600;
  margin-bottom: 20px !important;
  padding-left: 0;
}

#navfilter {
  .navrowtabs #tabslist a {
    color: $base-secondary-color;
  }

  .navorder a {
    color: $base-secondary-color;
  }

  .navsearch .navsearch-content {
    .input-group-btn button {
      margin-left: -5px !important;
      height: 34px !important;
      background: $base-secondary-color;
      margin-bottom: 0;
      border-top-left-radius: 0;
      border-bottom-left-radius: 0;

      &:hover {
        background-color: darken($base-secondary-color, 10%);
      }
    }

    .input-group label input {
      border-top-right-radius: 0;
      border-bottom-right-radius: 0;
    }
  }
}

.campaign {
  margin-top: 5rem !important;
  text-transform: uppercase;
  font-weight: 700;
  color: #42c69d;
  font-size: 2rem;
}

.rotate-me.blue {
  background: $base-secondary-color;
  border-color: $base-secondary-color;
  width: 1000vw;
}

.rotate-me.orange {
  background-color: #bf0e04;
  border-color: #bf0e04;
  color: #fff;
}

/*IDENTIFIANT OUBLIÉ*/
.view-remind #user-registration {
